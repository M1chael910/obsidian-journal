{
  "name": "obsidian-journal",
  "version": "1.4.0",
  "description": "Journal plugin for Obsidian (https://obsidian.md)",
  "main": "main.js",
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest",
    "ts-check": "tsc --noEmit -skipLibCheck",
    "changelog": "npx git-cliff --output CHANGELOG.md",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:srg-kostyrko/obsidian-journal.git"
  },
  "keywords": [
    "Obsidian",
    "plugin",
    "journal"
  ],
  "author": "Sergii Kostyrko",
  "license": "MIT",
  "dependencies": {
    "@flatten-js/interval-tree": "^1.1.3",
    "obsidian": "^1.6.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.9.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0-alpha.40",
    "@typescript-eslint/parser": "^8.0.0-alpha.39",
    "builtin-modules": "^4.0.0",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "git-cliff": "^2.4.0",
    "globals": "^15.9.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.9",
    "prettier": "^3.3.3",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.1.0",
    "vite": "^5.4.1",
    "vite-plugin-static-copy": "^1.0.6",
    "vitest": "^2.0.5"
  },
  "lint-staged": {
    "*.ts": "eslint",
    "*.{ts,mjs,js,css,md}": "prettier --write"
  }
}
